<mvc:View controllerName="com.bootcamp.sapui5.freestyle.controller.Home"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
	<VBox id="vTest">
    	<Text id="textPrueba" text="{i18n>textPruebai18n}"></Text>
		<Button id="btnBuscar" text="{i18n>textBuscar}" press="onPress"/>
	</VBox>

		<List
		headerText="Products" id="listaPrueba"
		headerLevel="H2"
		items="{
			path: 'ProductCollection>/'
		}" >
		<StandardListItem
			title="{ProductName}"
		id="SLI"/>
	</List>
	
    </Page>
</mvc:View>